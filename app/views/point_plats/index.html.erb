<h1>Listing point_plats</h1>

<table>
  <tr>
    <th>Lat</th>
    <th>Lon</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @point_plats.each do |point_plat| %>
  <tr>
    <td><%= point_plat.lat %></td>
    <td><%= point_plat.lon %></td>
    <td><%= link_to 'Show', point_plat %></td>
    <td><%= link_to 'Edit', edit_point_plat_path(point_plat) %></td>
    <td><%= link_to 'Destroy', point_plat, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Point plat', new_point_plat_path %>
